# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here
DISCORD_CLIENT_ID=your_discord_client_id_here
SUPPORT_CHANNEL_PREFIX=support-

# Admin Role IDs (comma-separated)
# To get role IDs:
# 1. Enable Developer Mode in Discord (User Settings > Advanced > Developer Mode)
# 2. Right-click the role in Server Settings > Roles
# 3. Click "Copy Role ID"
ADMIN_ROLE_IDS=123456789012345678,987654321098765432

# JIRA Configuration
JIRA_HOST=your_jira_instance.atlassian.net
JIRA_USERNAME=your_jira_email
JIRA_API_TOKEN=your_jira_api_token
JIRA_PROJECT_KEY=KEY

# Bot Configuration
BOT_COMMAND_PREFIX=!
MEMORY_RETENTION_MESSAGES=10  # Number of messages to keep in ephemeral memory
